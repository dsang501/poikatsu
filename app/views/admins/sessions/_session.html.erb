<div class="field form-group col-xs-7">
  <%= f.label :"メールアドレス", class: "col-sm-4 control-label" %>
  <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "col-sm-8 form-control-static bbb" %>
</div>

<div class="field form-group col-xs-7">
  <%= f.label :"パスワード", class: "col-sm-4 control-label" %>
  <%= f.password_field :password, autocomplete: "current-password", class: "col-sm-8 form-control-static bbb" %>
</div>
